{
  "name": "atlas",
  "version": "1.0.0",
  "description": "Marine decision support tool",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors -p && npm run flask-static-digest",
    "start": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-watch\" \"npm run flask-server\"",
    "webpack-watch": "NODE_ENV=debug webpack --mode development --watch",
    "flask-server": "flask run --host=0.0.0.0",
    "flask-static-digest": "flask digest compile",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craicerjack/atlas.git"
  },
  "author": "Carlos Tighe",
  "license": "Private",
  "engines": {
    "node": ">=12"
  },
  "bugs": {
    "url": "https://github.com/craicerjack/atlas/issues"
  },
  "homepage": "https://github.com/craicerjack/atlas#readme",
  "dependencies": {
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "chroma-js": "^2.1.0",
    "d3v4": "^4.2.2",
    "fs": "^0.0.1-security",
    "geotiff": "^0.3.6",
    "jquery": "^3.4.1",
    "leaflet": "^1.6.0",
    "leaflet-canvaslayer-field": "^1.4.1",
    "materialize-css": "1.0.0",
    "sidebar-v2": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.2.2",
    "file-loader": "^5.0.2",
    "manifest-revision-webpack-plugin": "^0.4.1",
    "mini-css-extract-plugin": "^0.9.0",
    "raw-loader": "^4.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  }
}
